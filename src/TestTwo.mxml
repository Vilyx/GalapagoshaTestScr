<?xml version="1.0" encoding="utf-8"?>
<s:WindowedApplication xmlns:fx="http://ns.adobe.com/mxml/2009"
					   xmlns:s="library://ns.adobe.com/flex/spark"
					   xmlns:mx="library://ns.adobe.com/flex/mx"
					   xmlns:ns1="*"
					   width="1080" height="900"
					   addedToStage="windowedapplication1_addedToStageHandler(event)"
					   currentState="VesRost" showStatusBar="false">
	
	<fx:Script>
		<![CDATA[
			protected function windowedapplication1_addedToStageHandler(event:Event):void
			{
				stage.scaleMode = StageScaleMode.SHOW_ALL;
			}
		]]>
	</fx:Script>
	
	<s:states>
		<s:State name="VesRost"/>
		<s:State name="Recipe"/>
		<s:State name="Ecuador"/>
	</s:states>
	<ns1:VesRostComp width="100%" height="100%" includeIn="VesRost"/>
	<ns1:Recipe  width="100%" height="100%" includeIn="Recipe" />
	<ns1:Ecuador  width="100%" height="100%" includeIn="Ecuador" />
</s:WindowedApplication>
